{% extends 'base.html.twig' %}

{% block body %}
      <div class="row mt-3">
        <div class="container">
        <div class="col-8">
        {{form_start(form)}}
        <div class="col-4">
          {{ form_row(form.name)}}
          {{ form_row(form.surname)}}
          {{ form_row(form.age)}}
          {{ form_row(form.email)}}
          {{ form_row(form.password)}}
        </div>
        {{ form_end(form)}}
        </div>
      </div>
  </div>
{% endblock %}